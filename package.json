{
  "name": "fluent-object-stream",
  "version": "1.0.0-rc.2",
  "description": "Facilitate transformation on nodeJS streams",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=16.10.0"
  },
  "scripts": {
    "build": "rm -Rf build lib && tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test && npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vdizengremel/fluent-object-stream.git"
  },
  "keywords": [
    "object",
    "stream",
    "readable",
    "transform"
  ],
  "author": "vdizengremel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vdizengremel/fluent-object-stream/issues"
  },
  "homepage": "https://github.com/vdizengremel/fluent-object-stream#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "semantic-release": "^24.2.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
