{
  "name": "object-stream",
  "version": "1.0.0",
  "description": "Facilitate transformation on nodeJS streams",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "scripts": {
    "build": "rm -Rf build lib && tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test && npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vdizengremel/object-stream.git"
  },
  "keywords": [
    "object",
    "stream",
    "readable",
    "transform"
  ],
  "author": "vdizengremel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vdizengremel/object-stream/issues"
  },
  "homepage": "https://github.com/vdizengremel/object-stream#readme",
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
