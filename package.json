{
  "name": "fluent-object-stream",
  "version": "1.0.0",
  "description": "Facilitate transformation on nodeJS streams",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=16.10.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --clean --tsconfig tsconfig.build.json",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext *.ts --fix",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test && npm run lint",
    "version": "git add -A src",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vdizengremel/fluent-object-stream.git"
  },
  "keywords": [
    "object",
    "stream",
    "readable",
    "transform"
  ],
  "author": "vdizengremel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vdizengremel/fluent-object-stream/issues"
  },
  "homepage": "https://github.com/vdizengremel/fluent-object-stream#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.38.0"
  }
}
